---
// biome-ignore lint/correctness/noUnusedImports: Layout component is used in template below
import Layout from "../layouts/Layout.astro";
---

<Layout title="AEM Client - Analytics">
	<div class="max-w-6xl">
		<div class="mb-8">
			<h1 class="text-4xl font-bold text-gray-800 mb-3">Analytics</h1>
			<p class="text-lg text-gray-600">
				Analytics dashboard and reporting for application insights.
			</p>
		</div>

		<div class="bg-white rounded-lg shadow-md border border-gray-200 p-8">
			<p class="text-gray-600 text-center">
				Analytics content will be added here.
			</p>
		</div>
	</div>
</Layout>
